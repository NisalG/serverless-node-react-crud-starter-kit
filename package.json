{
  "name": "aws-serverless-typescript-crud1",
  "version": "1.0.0",
  "description": "Serverless aws-nodejs-typescript template",
  "main": "serverless.ts",
  "scripts": {
    "start": "serverless offline start --allowCache --aws-profile default --stage dev",
    "test": "jest --config=jest.config.js",
    "test:dev": "jest --config=jest.config.js --watchAll",
    "deploy": "serverless deploy --verbose --stage dev --aws-profile default",
    "deploy:staging": "serverless deploy --stage staging --region us-east-1 --verbose",
    "deploy:ci": "serverless deploy --verbose --stage ",
    "deploy:func": "serverless deploy function --aws-profile default --stage dev --verbose --function",
    "logs": "serverless logs --aws-profile default --stage dev --function",
    "compile": "tsc",
    "debug": "node --inspect ./node_modules/.bin/serverless offline -s dev",
    "typeorm": "typeorm-ts-node-esm -d ./src/data/mySQL/data-source.ts",
    "db:init": "typeorm-ts-node-esm -d ./src/data/mySQL/data-source.ts migration:generate -n ",
    "db:migrate": "typeorm-ts-node-esm -d ./src/data/mySQL/data-source.ts migration:generate -n",
    "db:makemigrations": "./node_modules/sequelize-auto-migrations/bin/makemigration.js",
    "db:model": "sequelize model:create --name User --attributes firstName:string",
    "seed": "ts-node ./node_modules/typeorm-extension/dist/cli/index.js seed -d ./src/data/mySQL/data-source.ts -s ./src/data/mySQL/seeding/seeds/mainSeeder.ts",
    "lint": "eslint .",
    "prettier": "prettier --check .",
    "format": "prettier --write .",
    "serverless:validate": "serverless safeguards validate --aws-profile default --stage dev",
    "package": "serverless package --aws-profile default --stage dev",
    "domain": "serverless create_domain --aws-profile default --stage dev",
    "prune": "serverless prune -n 10",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=14.15.0"
  },
  "dependencies": {
    "@middy/core": "^2.5.3",
    "@middy/http-cors": "^3.1.0",
    "@middy/http-json-body-parser": "^2.5.3",
    "@middy/http-multipart-body-parser": "^3.1.1",
    "@middy/http-security-headers": "^3.0.3",
    "@types/jest": "^29.5.1",
    "amazon-cognito-identity-js": "^6.2.0",
    "aws-jwt-verify": "^4.0.0",
    "aws-lambda": "^1.0.7",
    "babel-jest": "^29.5.0",
    "class-validator": "^0.14.0",
    "jest": "^29.5.0",
    "jest-aws-sdk-mock": "^1.0.2",
    "mongoose": "^7.0.4",
    "mysql": "^2.18.1",
    "save": "^2.9.0",
    "stripe": "^12.9.0",
    "typeorm": "^0.3.6",
    "typeorm-extension": "^2.1.15",
    "uuid4": "^2.0.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@serverless/safeguards-plugin": "^1.2.1",
    "@serverless/typescript": "^3.0.0",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "esbuild": "^0.14.11",
    "eslint": "^8.43.0",
    "husky": "^8.0.3",
    "json-schema-to-ts": "^1.5.0",
    "serverless": "^3.27.0",
    "serverless-auto-swagger": "^2.5.1",
    "serverless-bundle": "^6.0.0",
    "serverless-domain-manager": "^7.0.3",
    "serverless-dotenv-plugin": "^6.0.0",
    "serverless-esbuild": "^1.23.3",
    "serverless-offline": "^8.8.0",
    "serverless-plugin-log-retention": "^2.0.0",
    "serverless-plugin-split-stacks": "^1.11.3",
    "serverless-plugin-warmup": "^8.0.0",
    "serverless-prune-plugin": "^2.0.1",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.3"
  },
  "author": "The serverless webpack authors (https://github.com/elastic-coders/serverless-webpack)",
  "license": "MIT"
}
